            <info compressed="false">https://raw.githubusercontent.com/Natsumi-x/natsumi-x.github.io/main/repo/zips/addons.xml</info>
            <checksum>https://raw.githubusercontent.com/Natsumi-x/natsumi-x.github.io/main/repo/zips/addons.xml.md5</checksum>
            <datadir zip="true">https://raw.githubusercontent.com/Natsumi-x/natsumi-x.github.io/main/repo/zips/</datadir>
        </dir>
    </extension>
    <extension point="xbmc.addon.metadata">
        <summary>vigilante-pancake repo</summary>
        <description>Wizard for Epic's Kodi Build</description>
        <disclaimer />
