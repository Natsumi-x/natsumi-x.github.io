    <import addon="xbmc.python" version="3.0.0" />
  </requires>
  <extension point="xbmc.python.pluginsource" library="default.py">
        <provides>video executable</provides>
  </extension>
  <extension point="xbmc.addon.metadata">
    <summary lang="en">Epic Kodi Wizard</summary>
    <description lang="en">Simple Kodi build</description>
	<website>http://EpicKodi.com</website>
    <platform>all</platform>
  </extension>
</addon><addon id="repository.vigilante-pancake" name="vigilante-pancake" version="1.0.0" provider-name="Natsumi-x">
